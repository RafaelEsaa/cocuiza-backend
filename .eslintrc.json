{
    "parserOptions": {
      "ecmaVersion": 2022
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "env": {
        "node": true,
        "jest": true,
        "es6": true
    },
    "rules": {
      "no-console": "warn",
      "indent": ["error", 4, { "SwitchCase": 1 }],
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "space-before-blocks": "error"
    }
}
